#!/bin/sh

echo "NR5G RU: Setup profile clock"
i2cset -f -y 10 0x69     0 0x54
i2cset -f -y 10 0x69     1 0xE4
i2cset -f -y 10 0x69     2 0x42
i2cset -f -y 10 0x69     3 0x15
i2cset -f -y 10 0x69     4 0x92
i2cset -f -y 10 0x69     5 0xED
i2cset -f -y 10 0x69     6 0x2D
i2cset -f -y 10 0x69     7 0x2A
i2cset -f -y 10 0x69     8 0x00
i2cset -f -y 10 0x69     9 0xC0
i2cset -f -y 10 0x69    10 0x00
i2cset -f -y 10 0x69    11 0x40
i2cset -f -y 10 0x69    19 0x29
i2cset -f -y 10 0x69    20 0x3E
i2cset -f -y 10 0x69    21 0xFF
i2cset -f -y 10 0x69    22 0xDF
i2cset -f -y 10 0x69    23 0x1F
i2cset -f -y 10 0x69    24 0x3F
i2cset -f -y 10 0x69    25 0xC0
i2cset -f -y 10 0x69    31 0x00
i2cset -f -y 10 0x69    32 0x00
i2cset -f -y 10 0x69    33 0x01
i2cset -f -y 10 0x69    34 0x00
i2cset -f -y 10 0x69    35 0x00
i2cset -f -y 10 0x69    36 0x01
i2cset -f -y 10 0x69    40 0xC0
i2cset -f -y 10 0x69    41 0x5D
i2cset -f -y 10 0x69    42 0x5F
i2cset -f -y 10 0x69    43 0x00
i2cset -f -y 10 0x69    44 0x02
i2cset -f -y 10 0x69    45 0xEA
i2cset -f -y 10 0x69    46 0x00
i2cset -f -y 10 0x69    47 0x15
i2cset -f -y 10 0x69    48 0xB5
i2cset -f -y 10 0x69    55 0x00
i2cset -f -y 10 0x69   131 0x1F
i2cset -f -y 10 0x69   132 0x02
i2cset -f -y 10 0x69   137 0x01
i2cset -f -y 10 0x69   138 0x0F
i2cset -f -y 10 0x69   139 0xFF
i2cset -f -y 10 0x69   142 0x00
i2cset -f -y 10 0x69   143 0x00
i2cset -f -y 10 0x69   136 0x40

echo "NR5G RU: Setup CPRI Core"
poke 0x80000000 0x01

echo "NR5G RU: Setting up system, ready to run"

